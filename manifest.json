{
    "manifest_version": 3,
    "name": "Facebook DarkMode",
    "description": "Change my Background color in my facebook",
    "version": "1.0",
    "icons": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.facebook.com/*"],
        "css": ["main.css"]
      }
    ]
  }
  